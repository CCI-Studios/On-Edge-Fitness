					&.expanded
					{
						background:url(../images/mobile-menu-item-arrow.png) no-repeat 96% 17px;
					}

					a 
					{
						color:white;
						padding-left:15px;
						border-top:1px solid rgba(255, 255, 255, 0.17);
						line-height:38px;

						&:hover,
						&:active
						{
							background:url(../images/mobile-menu-item-hover.png) repeat-x top;
							color:#c2e96b;
						}
					}

					ul 
					{
						display:block;
						position:static;
						width:100%;
						box-shadow:none;
						border-radius:0;
						padding:0;
						background-color:rgba(255,255,255,0.15);

						li 
						{
							border-top:1px solid rgba(50, 73, 0, 0.26);

							&.last 
							{
								border-bottom:0;

								a 
								{
									border-bottom:0;
								}
							}

							a 
							{
								color:white;
								padding-left:50px;
								border-top:1px solid rgba(255, 255, 255, 0.17);
								line-height:38px;

								&:hover,
								&:active
								{
									background:url(../images/mobile-menu-item-hover.png) repeat-x top;
								}
							}
						}
					}
				}
			}
		}
	}

	#block-menu-block-3
	{
		display:none;
	}

	#footer 
	{
		text-align:center;
		padding-bottom:80px;

		& > div 
		{
			background-image:none;

			& > div 
			{
				padding-right:0;
			}
		}

		#copyright,
		#site-by-cci
		{
			float:none;
			display:inline;
		}
	}

	#logo 
	{
		text-align:center;
	}

	#block-block-3 
	{
		&,
		div,
		p
		{
			display:inline-block;
			vertical-align:middle;
			margin:0;
		}

		a
		{
			display:inline-block;
			float:left;
		}
	}

	#block-menu-block-1
	{
		position:static;

		&,
		div,
		ul
		{
			display:inline-block;
			vertical-align:top;
		}
	}

	.region-header 
	{
		text-align:center;
		margin-top:-5px;
		padding-bottom:8px;
	}

	#slogan .text 
	{
		font-size:38px;
	}

	#widgets .views-row
	{
		width:auto;
		margin:0 10px 10px 10px!important;

		& > div 
		{
			min-height:0;
		}
	}

	#widgets2 
	{
		#block-block-1 
		{
			width:auto;
			margin:0 10px 10px 10px;
			float:none;
		}

		#block-block-2 
		{
			width:55%;
			margin-right:1%;
			margin-top:10px;

			a img 
			{
				width:auto;
			}
		}

		#block-menu-block-2
		{
			margin-top:0;
			width:44%;
			margin-top:10px;
		}
	}
}


@media screen and (max-width:540px)
{
	.front #block-system-main img 
	{
		display:none;
	}

	#slogan .text 
	{
		font-size:30px;
	}

	#widgets2 
	{

		#block-block-2 
		{
			width:100%;
			margin:10px;

			a 
			{
				display:block;
				text-align:center;

				img 
				{
					display:inline;
				}
			}
		}

		#block-menu-block-2
		{
			width:100%;
			margin:10px;
		}
	}

	#widgets2 > div:after
	{
		display:none;
		content:none;
	}
}


@media screen and (max-width:480px)
{

	#slogan .text 
	{
		font-size:22px;
	}

}

